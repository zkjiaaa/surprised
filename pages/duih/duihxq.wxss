.item-price image {
    width: 32rpx;
    margin-right: 10rpx;
    height: 32rpx;
}

.leftnico {
    -webkit-box-pack: start!important;
    justify-content: flex-start!important;
}

.leftnico text {
    margin-right: 20rpx;
}

.caoz-item {
    height: 60rpx;
    width: 200rpx;
    background-color: #f63;
    color: #fff;
    font-size: 28rpx;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    margin-right: 20rpx;
    border-radius: 60rpx;
}

.caoz {
    display: flex;
    background: #fff;
    bottom: 0;
    margin-top: 40rpx;
    left: 0;
    width: 100%;
    height: 80rpx;
    -webkit-box-pack: end;
    justify-content: flex-end;
}

.body {
    box-sizing: border-box;
    padding: 0 30rpx;
}

.nico {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    height: 80rpx;
    line-height: 80rpx;
    color: #333;
    border-bottom: 1rpx solid #f5f5f5;
    font-size: 28rpx;
    color: #333;
}

.card-jianj {
    width: 100%;
    height: 200rpx;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    font-size: 42rpx;
    font-weight: 700;
    color: #f63;
    border-bottom: 20rpx solid #f5f5f5;
}

.title {
    border-bottom: 20rpx solid #f5f5f5;
}

.shu-left {
    display: flex;
    width: 100%;
}

.shu .item-title {
    text-align: left;
    white-space: normal;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    line-clamp: 2;
    -webkit-box-orient: vertical;
}

.shu .item-price {
    text-align: left;
}

.shu {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: justify;
    justify-content: space-between;
    padding: 20rpx;
    box-sizing: border-box;
    padding-top: 10rpx;
    -webkit-box-flex: 1;
    flex: 1;
    width: 390rpx;
}

.item-image-shu {
    width: 200rpx;
    height: 200rpx;
}

.item-image-shu image {
    width: 200rpx;
    border-radius: 10rpx;
    height: 200rpx;
}

.item-shu {
    display: flex;
    box-sizing: border-box;
    padding: 20rpx;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    width: 100%;
}

.item-price {
    color: #f63;
    font-size: 28rpx;
    text-align: center;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.item-title {
    white-space: nowrap;
    overflow: hidden;
    text-align: center;
    font-size: 28rpx;
    margin-top: 8rpx;
    text-overflow: ellipsis;
}

.item-image image {
    width: 210rpx;
    height: 210rpx;
}

.item-image {
    width: 210rpx;
    height: 210rpx;
}

.item {
    width: 210rpx;
    margin: 30rpx 20rpx;
    margin-top: 20rpx;
}