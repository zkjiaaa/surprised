.flexb {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
}

.header {
    width: 100%;
    height: 300rpx;
    background-color: #7336db;
    padding: 1rpx;
    position: relative;
}

.jiang {
    position: absolute;
    top: 150rpx;
    left: 30rpx;
}

.jiangpq {
    font-size: 42rpx;
    color: #fff;
}

.shengy {
    font-size: 30rpx;
    color: #fff;
    margin-top: 12rpx;
}

.shengy image {
    width: 36rpx;
    height: 36rpx;
    margin-bottom: -6rpx;
}

.box {
    background-color: #fff;
    margin: 24rpx 30rpx;
    border-radius: 18rpx;
    padding: 24rpx;
}

.img {
