.more {
    width: 596rpx;
    height: 108rpx;
    text-align: center;
    background: hsla(0,0%,88.2%,.2);
    border-radius: 10rpx;
    font-size: 36rpx;
    margin: 0 auto;
    margin-top: 38rpx;
    margin-bottom: 68rpx;
    color: #fff;
    line-height: 108rpx;
    animation: changes .5s .2s linear infinite alternate;
}

@-webkit-keyframes changes {
    from {
        transform: scale(1);
    }

    to {
        transform: scale(1.15);
    }
}

@keyframes changes {
    from {
        transform: scale(1);
    }

    to {
        transform: scale(1.15);
    }
}

.getmore {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 30rpx;
    color: #fff;
    text-align: center;
    margin-top: -20rpx;
    margin-bottom: 20rpx;
}

.drac-chakan {
    position: absolute;
    width: 134rpx;
    height: 56rpx;
    color: #fff;
    font-size: 24rpx;
    color: #fff;
    background-color: #f63;
    border-radius: 56rpx;
    text-align: center;
    line-height: 56rpx;
    bottom: 40rpx;
    right: 30rpx;
}

.drac-title {
    position: absolute;
    bottom: 0;
    z-index: 10;
    padding: 28rpx 28rpx;
    box-sizing: border-box;
    height: 140rpx;
    color: #333;
    width: 100%;
    border-bottom-left-radius: 20rpx;
    border-bottom-right-radius: 20rpx;
    background: #fff;
}

.drac-title-time {
    font-size: 28rpx;
    margin-top: 10rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 420rpx;
}

.drac-title-item {
    font-size: 32rpx;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 420rpx;
}

.drac-img {
    position: absolute;
    width: 100%;
    height: 100%;
}

.drac {
    width: 690rpx;
    height: 450rpx;
    background-color: #fff;
    overflow: hidden;
    margin: 0 auto 42rpx auto;
    background: none;
    box-shadow: 0 0 5px 0 rgba(0,0,0,.1);
    border-radius: 20rpx;
    position: relative;
}

.hezi {
    position: fixed;
    top: 360rpx;
    right: 20rpx;
    width: 140rpx;
    border-radius: 10rpx;
    text-align: center;
}

.anniu-item {
    width: 80rpx;
    height: 80rpx;
    padding: 0;
    margin: 0;
    margin-bottom: 20rpx;
    line-height: 40rpx;
    font-size: 22rpx;
    display: flex;
    background: none;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.anniu-item::after {
    border: none;
}

.anniu-img {
    width: 80rpx;
    border-radius: 50%;
    height: 80rpx;
}

.anniu {
    position: fixed;
    bottom: 120rpx;
    right: 20rpx;
    z-index: 999;
}

.moreplay {
    box-sizing: border-box;
    padding: 10rpx;
    color: #fff;
    border-radius: 8rpx;
}

.hezi image {
    width: 70rpx;
    height: 70rpx;
}

.banner {
    width: 100%;
    position: relative;
    min-height: 80rpx;
}

.banner image {
    width: 100%;
}

.content {
    background-color: #581adf;
    min-height: 100vh;
}

.luck-rukou {
    width: 690rpx;
    margin: 30rpx;
}

.luck-box1 {
    font-size: 36rpx;
    font-weight: 700;
    color: #fff;
    width: 330rpx;
    height: 230rpx;
    border-radius: 18rpx;
    box-sizing: border-box;
    margin-bottom: 30rpx;
    overflow: hidden;
}

.luck-box1 image {
    width: 100%;
    height: 100%;
}

.luck {
    margin: 30rpx;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    width: 690rpx;
    flex-wrap: wrap;
    color: #fff;
}

.tui-notice-board {
    width: 700rpx;
    z-index: 999;
    margin: 0 auto;
    padding-right: 25rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    height: 60rpx;
    background: rgba(0,0,0,.5);
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    position: absolute;
    margin-left: 25rpx;
    top: 20rpx;
    border-radius: 10rpx;
}

.tui-scorll-view {
    -webkit-box-flex: 1;
    flex: 1;
    line-height: 60rpx;
    white-space: nowrap;
    overflow: hidden;
    color: #fff;
    height: 100%;
}

.datirukou {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    width: 690rpx;
    margin: 20rpx auto;
    height: 120rpx;
    font-size: 34rpx;
    border-radius: 10rpx;
    -webkit-box-align: center;
    align-items: center;
    letter-spacing: 1rpx;
    color: #fff;
}

.datirukou-item1 {
    width: 440rpx;
    text-align: center;
    background: rgba(0,0,0,.2);
    height: 100%;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    display: flex;
    letter-spacing: 4rpx;
    border-radius: 10rpx;
}

.datirukou-item2 {
    width: 220rpx;
    background: rgba(0,0,0,.2);
    text-align: center;
    height: 100%;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    display: flex;
    border-radius: 10rpx;
}