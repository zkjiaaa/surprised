page {
    background: #f2f2f2;
}

.content {
    text-align: center;
}

.touxiang {
    width: 450rpx;
    height: 450rpx;
    box-sizing: border-box;
    padding: 16rpx;
    border-radius: 10rpx;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    background: #fff;
    margin: 150rpx;
}

.touxiang image {
    width: 100%;
}

.dibu {
    width: 100%;
    height: 100rpx;
    background: #fff;
    box-sizing: border-box;
    display: flex;
    position: fixed;
    bottom: 0;
    left: 0;
}

.item {
    width: 33%;
    height: 100rpx;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.item-img {
    width: 40rpx;
    height: 40rpx;
}

.item-img image {
    width: 40rpx;
    height: 40rpx;
}

.item-name {
    color: #333;
    font-size: 26rpx;
    margin-top: 10rpx;
}