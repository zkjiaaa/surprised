.tuij-price {
    color: #f52f3e;
}

.tuij-xx1 {
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    line-clamp: 2;
    -webkit-box-orient: vertical;
}

.tuij-xx {
    -webkit-box-flex: 1;
    flex: 1;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: justify;
    justify-content: space-between;
    box-sizing: border-box;
    padding: 20rpx;
    padding-top: 10rpx;
    font-size: 26rpx;
}

.tuijgood image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 8rpx;
}

.tuijgood {
    display: flex;
    box-sizing: border-box;
    padding: 20rpx;
    margin-top: 20rpx;
    background: #f2f3f4;
    border-radius: 10rpx;
}

.fab {
    background: #f63;
    color: #fff;
    font-size: 30rpx;
    width: 200rpx;
    text-align: center;
    line-height: 60rpx;
    height: 60rpx;
    position: fixed;
    left: 50%;
    margin-left: -100rpx;
    bottom: 20rpx;
    border-radius: 60rpx;
}

.text-image {
    display: flex;
    margin-top: 20rpx;
}

.text-image image {
    border-radius: 6rpx;
    width: 205rpx;
    height: 205rpx;
    margin-right: 20rpx;
}

.active {
    display: block!important;
    margin-bottom: 30rpx;
}

.text-quanb {
    font-size: 28rpx;
    margin-top: 30rpx;
    color: #0151ff;
    margin-bottom: 20rpx;
}

.text-text {
    font-size: 28rpx;
    color: #666;
    margin-top: 10rpx;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    line-clamp: 2;
    -webkit-box-orient: vertical;
}

.text-title {
    font-size: 30rpx;
    color: #000;
    font-weight: 700;
}

.item-text {
    margin-top: 30rpx;
}

.time1 {
    font-size: 26rpx;
    color: #999;
}

.user-name {
    color: #333;
    font-size: 30rpx;
    margin-left: 20rpx;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    box-sizing: border-box;
    padding: 4rpx 0;
}

.user-img {
    width: 80rpx;
    height: 80rpx;
}

.user-img image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.item-title {
    display: flex;
}

.item {
    width: 690rpx;
    margin: 0 auto;
    background: #fff;
    box-sizing: border-box;
    padding: 20rpx;
    margin-top: 20rpx;
    border-radius: 10rpx;
}

page {
    background: #f5f5f5;
}