.caoz {
    width: 100%;
    padding: 0 30rpx;
    box-sizing: border-box;
    display: flex;
}

.wrap {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #fff;
    z-index: 200;
}

.content {
    text-align: center;
    background: #fff!important;
}

.header {
    text-align: center;
    height: 300rpx;
    line-height: 450rpx;
    background: #fff;
    -webkit-box-pack: center;
    justify-content: center;
}

.header image {
    width: 100rpx;
    height: 100rpx;
}

.content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}

.bottom2 {
    border-radius: 80rpx;
    margin: 70rpx auto;
    font-size: 32rpx;
    width: 270rpx;
    color: #f63;
}

.bottom2:after {
    border-radius: 100rpx;
}

.bottom {
    border-radius: 80rpx;
    margin: 70rpx auto;
    font-size: 32rpx;
    width: 270rpx;
    background: #f63;
}

.bottom::after {
    width: 200%;
    height: 200%;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    font-size: 68rpx;
}